{
  "name": "svgclean",
  "private": true,
  "version": "0.1.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "pnpm tauri dev",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:mac": "pnpm build && . $HOME/.cargo/env && pnpm tauri build --target aarch64-apple-darwin",
    "build:win": "pnpm build && pnpm tauri build --target x86_64-pc-windows-msvc",
    "build:linux": "pnpm build && pnpm tauri build --target x86_64-unknown-linux-gnu",
    "build:all": "pnpm build && . $HOME/.cargo/env && pnpm tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-opener": "^2",
    "@vueuse/core": "^14.1.0",
    "ant-design-vue": "^4.2.6",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.3"
  }
}
